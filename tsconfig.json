{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2021",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "@common/*": ["./src/common/*"],
      "@user/*": ["./src/user/*"],
      "@user": ["./src/user"],
      "@rewards/*": ["./src/rewards/*"],
      "@rewards": ["./src/rewards"],
      "@portfolio/*": ["./src/portfolio/*"],
      "@portfolio": ["./src/portfolio"],
      "@auth/*": ["./src/auth/*"],
      "@auth": ["./src/auth"],
      "@core/*": ["./src/core/*"],
      "@core": ["./src/core"],
      "@tournament/*": ["./src/tournament/*"],
      "@tournament": ["./src/tournament"],
      "@inventory/*": ["./src/inventory/*"],
      "@inventory": ["./src/inventory"],
      "@card/*": ["./src/card/*"],
      "@card": ["./src/card"],
      "@marketplace/*": ["./src/marketplace/*"],
      "@marketplace": ["./src/marketplace"],
      "@coin/*": ["./src/coin/*"],
      "@coin": ["./src/coin"],
      "@offer/*": ["./src/offer/*"],
      "@offer": ["./src/offer"],
      "@events/*": ["./src/events/*"],
      "@events": ["./src/events"],
      "@quests/*": ["./src/quests/*"],
      "@quests": ["./src/quests"],
      "@quests-processing/*": ["./src/quests-processing/*"],
      "@quests-processing": ["./src/quests-processing"],
      "@publishers/*": ["./src/publishers/*"],
      "@publishers": ["./src/publishers"],
      "@consumers/*": ["./src/consumers/*"],
      "@consumers": ["./src/consumers"],
      "@sqs-consumers/*": ["./src/sqs-consumers/*"],
      "@sqs-consumers": ["./src/sqs-consumers"],
      "@sns-publishers/*": ["./src/sns-publishers/*"],
      "@sns-publishers": ["./src/sns-publishers"],
      "@sqs/*": ["./src/sqs/*"],
      "@sqs": ["./src/sqs"],
      "@sns/*": ["./src/sns/*"],
      "@sns": ["./src/sns"],
      "@lock/*": ["./src/lock/*"],
      "@lock": ["./src/lock"],
      "@idempotency/*": ["./src/idempotency/*"],
      "@idempotency": ["./src/idempotency"],
      "@deduplication": ["./src/deduplication"],
      "@deduplication/*": ["./src/deduplication/*"],
      "@redis-lock/*": ["./src/redis-lock/*"],
      "@redis-lock": ["./src/redis-lock"],
      "@cache/*": ["./src/cache/*"],
      "@cache": ["./src/cache"]
    }
  }
}
